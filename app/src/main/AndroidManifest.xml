<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.self.projectmanager" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Light" >
        <activity android:name=".activity.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ProjectAddActivity" />
        <activity android:name=".activity.ProjectDetailActivity" />
        <activity android:name=".activity.MemberAddActivity" />
        <activity android:name=".activity.MemberDetailActivity" />
        <activity android:name=".activity.MemberSelectorActivity" />
        <activity android:name=".activity.ProjectSelectorActivity" />
        <activity android:name=".activity.SignInActivity" />
        <activity android:name=".activity.AdvanceActivity" />
    </application>

</manifest>
